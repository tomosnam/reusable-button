{
  "name": "reusable-button",
  "private": true,
  "version": "1.0.0",
  "description": "Reusable button component for React.js",
  "scripts": {
    "dev:build": "node esbuild.dev.config.mjs",
    "dev:serve": "servor public index.html 3000 --reload --browse",
    "build": "node esbuild.prod.config.mjs",
    "lint": "eslint --ext=.jsx,.js,.tsx,.ts  .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "as": "node esbuild.config.mjs -w"
